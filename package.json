{
  "name": "streaming-slack-notify",
  "version": "1.0.0",
  "description": "send slack notification with advanced message format",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "build": "ncc build --minify --out dist src/index.js",
    "format": "prettier --check **/*.{js,json,yml,md}",
    "format:write": "prettier --write -l **/*.{js,json,yml,md}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techpivot/streaming-slack-notify.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Slack",
    "Notify",
    "Streaming"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true
  },
  "author": "TechPivot",
  "license": "MIT",
  "homepage": "https://github.com/techpivot/streaming-slack-notifyr#readme",
  "dependencies": {
    "@actions/artifact": "^0.3.1",
    "@actions/github": "^2.1.1"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "prettier": "2.0.5"
  }
}
