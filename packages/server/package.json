{
  "name": "@techpivot/streaming-slack-notify-server",
  "version": "0.0.0",
  "description": "Backend server that polls SQS for new jobs and queries GitHub action status, posting results to Slack",
  "license": "MIT",
  "homepage": "https://github.com/techpivot/streaming-slack-notify#readme",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/techpivot/streaming-slack-notify.git",
    "directory": "packages/server"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "scripts": {
    "build": "tsc --build --incremental && ncc build --no-source-map-register --out dist src/index.ts",
    "start": "yarn build && node dist"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  },
  "author": "TechPivot",
  "keywords": [
    "TechPivot",
    "GitHub",
    "Actions",
    "Slack",
    "Notify",
    "Streaming",
    "Server"
  ],
  "dependencies": {
    "@actions/github": "^2.2.0",
    "@octokit/rest": "^17.9.2",
    "@slack/web-api": "^5.9.0",
    "@techpivot/streaming-slack-notify-common": "0.0.0",
    "debug": "^4.1.1",
    "io-ts": "^2.2.3",
    "fp-ts": "^2.6.1"
  },
  "devDependencies": {
    "@slack/types": "^1.6.0",
    "@types/debug": "^4.1.5"
  }
}
