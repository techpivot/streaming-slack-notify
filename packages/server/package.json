{
  "name": "techpivot-streaming-slack-notify-server",
  "version": "0.0.0",
  "description": "Backend server that polls SQS for new jobs and queries GitHub action status, posting results to Slack",
  "repository": {
    "type": "git",
    "url": "https://github.com/techpivot/streaming-slack-notify.git",
    "directory": "packages/server"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --incremental",
    "start": "node dist",
    "format:check": "prettier --check **/*.{ts,js,json,yml,md}",
    "format": "prettier --write -l **/*.{ts,js,json,yml,md}"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  },
  "author": "TechPivot",
  "keywords": [
    "TechPivot",
    "GitHub",
    "Actions",
    "Slack",
    "Notify",
    "Streaming",
    "Server"
  ],
  "license": "MIT",
  "homepage": "https://github.com/techpivot/streaming-slack-notify#readme",
  "dependencies": {
    "@octokit/rest": "^17.9.2",
    "@slack/web-api": "^5.9.0",
    "aws-sdk": "^2.679.0",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@octokit/types": "^4.0.2",
    "@slack/types": "^1.6.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "typescript": "^3.8"
  }
}
