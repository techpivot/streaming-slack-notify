{
  "name": "techpivot-streaming-slack-notify-github-action",
  "version": "0.0.0",
  "description": "The GitHub Action that is used for initiating the streaming notifications to Slack",
  "repository": {
    "type": "git",
    "url": "https://github.com/techpivot/streaming-slack-notify.git",
    "directory": "packages/github-action"
  },
  "main": "index.ts",
  "scripts": {
    "build": "ncc build --minify --out dist src/index.ts",
    "format:check": "prettier --check **/*.{ts,js,json,yml,md}",
    "format": "prettier --write -l **/*.{ts,js,json,yml,md}"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  },
  "author": "TechPivot",
  "keywords": [
    "TechPivot",
    "GitHub",
    "Actions",
    "Slack",
    "Notify",
    "Streaming"
  ],
  "license": "MIT",
  "homepage": "https://github.com/techpivot/streaming-slack-notify#readme",
  "dependencies": {
    "@actions/artifact": "^0.3.1",
    "@actions/core": "^1.2.4",
    "@actions/github": "^2.1.1",
    "@octokit/webhooks": "7.4.0",
    "@slack/web-api": "5.8.1"
  },
  "devDependencies": {
    "@slack/types": "^1.6.0",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "typescript": "^3.6.4"
  }
}
